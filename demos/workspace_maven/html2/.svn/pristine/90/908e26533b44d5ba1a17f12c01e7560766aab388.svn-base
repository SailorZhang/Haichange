<script type="text/javascript">      
	$(document).ready(function(){
		/* 当鼠标移到表格上是，当前一行背景变色 */
		$(".table_list tr td").mouseover(function(){
			$(this).parent().find("td").css("background-color","#d5f4fe");
		});
		/* 当鼠标在表格上移动时，离开的那一行背景恢复 */
		$(".table_list tr td").mouseout(function(){
			var bgc = $(this).parent().attr("bg");
			$(this).parent().find("td").css("background-color",bgc);
		});
		var color="#f7f7f7"
		/* 改变偶数行背景色 */
		$(".table_list tr:even td").css("background-color",color);
		/* 把背景色保存到属性中 */
		$(".table_list tr:even").attr("bg",color);
		$(".table_list tr:odd").attr("bg","#fff");
      });
</script>
